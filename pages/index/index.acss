.page-title {
    position: sticky;
    top: 0;
    z-index: 999;
    width: 100%;
    background: #FFFFFF;
    box-sizing: border-box;
    padding: 0 30rpx;
    display: flex;
    align-items: center;
    overflow: hidden;
}

.locationBox {
    display: flex;
    align-items: center;
    line-height: 0;
    max-width: 375rpx;
}

.locationBox .locationBox-left {
    width: 32rpx;
    height: 32rpx;
    background: url('data:image/png;base64,') no-repeat center center;
    background-size: 100%;
}

.locationBox .locationBox-center {
    flex: 1;
    line-height: normal;
    max-width: 345rpx;
    color: #000000;
    font-size: 30rpx;
    font-weight: bold;
    width: 100%;
    box-sizing: border-box;
    padding-left: 2rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    font-family: PingFang SC;
    color: #000;
}

.locationBox .locationBox-right {
    width: 28rpx;
    height: 28rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 100%;
    vertical-align: bottom;
}